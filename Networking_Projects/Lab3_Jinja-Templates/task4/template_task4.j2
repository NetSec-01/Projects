{% for n in range(10) %}
 interface GigabitEthernet0/{{ (n+1) }}
  {% if n+1 == 1 %}
   description {{interface.description[0]}}
   Switch Port mode Trunk
  {% else %}
    description {{interface.description[1]}}
    switchport mode access
    switchport access vlan {{ interface.vlan}}
  {% endif %}
{% endfor %}
